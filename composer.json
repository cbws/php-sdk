{
    "name": "cbws/php-cbws",
    "type": "library",
    "license": "Apache-2.0",
    "description": "Client libraries for the CBWS platform",
    "authors": [
        {
            "name": "CBWS",
            "email": "cbws@cbws.xyz"
        }
    ],
    "require": {
        "cbws/php-cbws-grpc": "^0.7.0",
        "ext-grpc": "^1.29",
        "league/oauth2-client": "^2.4",
        "symfony/yaml": "^5.1",
        "lcobucci/jwt": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~8 || ~0 || ~0 || ~0 || ~0 || ~0 || ~0 || ~0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:cbws/php-cbws-grpc.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cbws\\API\\": "src"
        },
        "files": ["src/OAuth2/Functional/RandomStr.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Cbws\\API\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --testsuite Unit"
    }
}
